mysql> select w.firstname as firstname, w.lastname as lastname, player.role as role, team.name as team from wizard w
    -> join player on w.id=player.wizard_id 
    -> join team on team.id=player.team_id
    -> order by team.name, player.role, w.lastname, w.firstname;
